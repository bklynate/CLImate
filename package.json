  "scripts": {
    "start:trace": "NODE_OPTIONS='--trace-deprecation' npx tsx index.ts",
    "dev": "echo '{\"messages\": []}' > db.json && OBJC_DISABLE_INITIALIZE_FORK_SAFETY=YES ORT_LOG_SEVERITY_LEVEL=3 ORT_LOGGING_LEVEL=3 NODE_OPTIONS='--no-deprecation' npx tsx index.ts",
