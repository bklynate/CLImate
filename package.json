    "dev:langchain": "NODE_OPTIONS='--no-deprecation' npx tsx src-langchain/index.ts",
    "dev:langchain:reset": "echo '{\"messages\": [], \"threadId\": null}' > db-langchain.json && npm run dev:langchain",
    "cli": "NODE_OPTIONS='--no-deprecation' npx tsx src-langchain/cli.ts",
    "cli:reset": "echo '{\"messages\": [], \"threadId\": null}' > db-langchain.json && npm run cli",
    "test": "vitest run",
